{
  "name": "aws-amplify-cljs-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.4.0",
    "@aws-amplify/datastore": "^4.5.0",
    "@aws-amplify/ui-components": "^1.9.40",
    "@aws-amplify/ui-react": "^5.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.133",
    "@mui/material": "^5.13.4",
    "@mui/styles": "^5.13.2",
    "@mui/system": "^5.13.2",
    "@rjsf/core": "^5.8.1",
    "@rjsf/mui": "^5.8.1",
    "@rjsf/utils": "^5.8.1",
    "@rjsf/validator-ajv6": "^5.8.1",
    "@sentry/react": "^7.54.0",
    "@sentry/tracing": "^7.54.0",
    "@tiptap/extension-collaboration": "^2.0.3",
    "@tiptap/pm": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "aws-amplify": "^5.2.5",
    "howler": "^2.2.3",
    "react": "^18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-ios-pwa-prompt": "^1.8.4",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.12.1",
    "use-sync-external-store": "^1.2.0",
    "web-vitals": "^3.3.2",
    "workbox-precaching": "^7.0.0",
    "y-prosemirror": "^1.2.1",
    "y-webrtc": "^10.2.5",
    "yjs": "^13.6.2"
  },
  "scripts": {
    "webrtc": "PORT=4444 node ./scripts/server.mjs",
    "update-email-templates": "nbb scripts/update_email_templates.cljs",
    "update": "ncu -u && rm -rf node_modules && rm yarn.lock && yarn",
    "nuke": "rm -rf dist/js .shadow-cljs node_modules yarn.lock target karma/js report.html",
    "clear": "rm -rf target karma/js report.html",
    "lint": "clj-kondo --lint ./ --dependencies --parallel --copy-configs",
    "pretty": "prettier --write '**/*.*'",
    "cljfmt": "clojure -Sdeps '{:deps {cljfmt {:mvn/version \"0.9.2\"}}}' -m cljfmt.main fix",
    "format": "yarn cljfmt && yarn pretty && yarn lint",
    "watch": "shadow-cljs watch app karma-test",
    "ci": "shadow-cljs release app && shadow-cljs compile karma-test && webpack && karma start --single-run --reporters junit,dots",
    "karma": "karma start",
    "build": "shadow-cljs release app",
    "build-verbose": "shadow-cljs release --verbose app",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "debug-build": "shadow-cljs release app --debug",
    "dev-build": "shadow-cljs compile app",
    "shadow-cljs-server": "shadow-cljs server",
    "webpack": "webpack --watch",
    "cypress": "CYPRESS_CONFIG=$(aws secretsmanager get-secret-value --secret-id fate/cypress/config --query SecretString --output text) cypress open",
    "cypress:run": "CYPRESS_CONFIG=$(aws secretsmanager get-secret-value --secret-id fate/cypress/config --query SecretString --output text) cypress run  --headless --browser chrome"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-amplify/cli": "^12.0.3",
    "@aws-sdk/client-cognito-identity-provider": "^3.350.0",
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/client-ses": "^3.350.0",
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/react": "^14.0.0",
    "babel-loader": "^9.1.2",
    "create-react-class": "^15.7.0",
    "cypress": "^12.14.0",
    "gmail-tester": "^1.3.8",
    "html-beautifier-webpack-plugin": "^1.0.6",
    "html-webpack-plugin": "^5.5.3",
    "karma": "6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cljs-test": "^0.1.0",
    "karma-junit-reporter": "^2.0.1",
    "lodash": "^4.17.21",
    "nbb": "^1.2.174",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "promise-poller": "^1.9.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "shadow-cljs": "2.23.3",
    "start-server-and-test": "^2.0.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  }
}
